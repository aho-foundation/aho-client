{
  "productName": "app",
  "version": "0.0.0",
  "identifier": "dev.tauri.app",
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "devUrl": "https://localhost:3000"
  },
  "app": {
    "windows": [
      {
        "title": "app",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "fs": {
      "scope": {
        "allow": ["**/*"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "all": true
      }
    }
  }
}
